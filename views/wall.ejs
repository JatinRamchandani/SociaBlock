<html>
    <head>
        <style>
            ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow:hidden;
        background-color:rgb(18, 67, 214);
      }

      li{
        float: left;
      }

      li a{
        display:block;
        color:white;
        text-align: center;
        text-decoration: none;
        padding: 14px 16px;

      }

      li a:hover{
        background-color: rgb(4, 39, 145);
      }
      .active {
        background-color: #4CAF50;
      }

      .navtext{
        font-family: Arial, Helvetica, sans-serif;
        color: white;
      }
        </style>

    </head>
    <% username.forEach(function (user) { %>
        <ul>
            <li><a href="#" class="navtext">SociaBlock</a></li>
            <li>   </li>
            <li><a href="/login1/<%= user.Email %>/<%= user.Password %>" class="navtext">My Profile</a></li>
            <li><a href="/addPosts" class="navtext">Add Post</a></li>
        </ul>
    <h1>Hi! <%= user.FName %></h1><br>
    
    

    <% }) %>

    <br><br>
    <iframe src="/wallposts" width="100%" height="100%"   style="border:none"></iframe>

    
</html>